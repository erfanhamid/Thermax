            RemoveError();
            ValidateOnAddBtnClick();


            if (isValid == true) {
                if (parseFloat($('#AvailableQTY').val()) < parseFloat($('#ItemQty').val())) {
                    alert("Issue Quantity can not be greater than Available Quantity ");
